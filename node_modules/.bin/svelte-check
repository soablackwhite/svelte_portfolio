#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/omie/Documents/Work/svelte/svelte_build/node_modules/.pnpm/svelte-check@3.6.3_svelte@4.2.9/node_modules/svelte-check/bin/node_modules:/Users/omie/Documents/Work/svelte/svelte_build/node_modules/.pnpm/svelte-check@3.6.3_svelte@4.2.9/node_modules/svelte-check/node_modules:/Users/omie/Documents/Work/svelte/svelte_build/node_modules/.pnpm/svelte-check@3.6.3_svelte@4.2.9/node_modules:/Users/omie/Documents/Work/svelte/svelte_build/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/omie/Documents/Work/svelte/svelte_build/node_modules/.pnpm/svelte-check@3.6.3_svelte@4.2.9/node_modules/svelte-check/bin/node_modules:/Users/omie/Documents/Work/svelte/svelte_build/node_modules/.pnpm/svelte-check@3.6.3_svelte@4.2.9/node_modules/svelte-check/node_modules:/Users/omie/Documents/Work/svelte/svelte_build/node_modules/.pnpm/svelte-check@3.6.3_svelte@4.2.9/node_modules:/Users/omie/Documents/Work/svelte/svelte_build/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
